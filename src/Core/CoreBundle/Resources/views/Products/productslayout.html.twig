{% extends '::base.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}"/>
<link rel="stylesheet" type="text/css" href="{{ asset('css/jQuery-ui/jquery-ui.min.css') }}"/>

{% endblock stylesheets %}
{% block body %}
    
    <div class="container-fluid">
        {% block header %}
        {% include 'CoreCoreBundle:Default:header.html.twig' %}
        {% endblock header %}
    </div>
    <div class="container">
        {% block filter %}
        {% include 'CoreCoreBundle:Products:filter.html.twig' %}
        {% endblock filter %}
        {% block listProduct %}
            <div class="col-xs-12 col-md-8" id="listProduct">
            </div>
        {% endblock listProduct%}
    </div>
    <div class="container-fluid">
        {% block footer %}
        {% include 'CoreCoreBundle:Default:footer.html.twig' %}
        {% endblock footer %}
     </div>



{% endblock body %}
{% block javascripts %}
{{ parent() }}

<script src="{{ asset('js/jquery-ui.min.js') }}"></script>
<script src="{{ asset('js/Default/ajax.js') }}"></script>
<script src="{{ asset('js/Front/Product/filter.js') }}"></script>
<script src="{{ asset('js/Front/Product/product.js') }}"></script>

{% endblock %}
