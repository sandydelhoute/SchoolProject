{% extends '::base.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}"/>

{% endblock stylesheets %}
{% block body %}
    
    <div class="container-fluid">
        {% block header %}
        {% include 'CoreCoreBundle:Default:header.html.twig' %}
        {% endblock header %}
    </div>
    <div class="container">
        {% block tunnel %}
            {% include 'CoreCoreBundle:Commande:commande.html.twig' %}
        {% endblock tunnel %}
        {% if listOrderLine|length > 0 %}
            {% block resume %}
                {% include 'CoreCoreBundle:Commande:detailpanier.html.twig' %}
            {% endblock resume%}
        {% endif %}
    </div>
    <div class="container-fluid">
        {% block footer %}
        {% include 'CoreCoreBundle:Default:footer.html.twig' %}
        {% endblock footer %}
     </div>



{% endblock body %}
