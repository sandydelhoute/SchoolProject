{% extends '::base.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}"/>

{% endblock stylesheets %}
{% block body %}
    
    <div class="container-fluid">
        {% block header %}
        {% include 'CoreCoreBundle:Default:header.html.twig' %}
        {% endblock header %}
    </div>
    <div class="container">
        {% block tunnel %}
            {% include 'CoreCoreBundle:Commande:commande.html.twig' %}
        {% endblock tunnel %}
        {% if listOrderLine|length > 0 %}
            {% block resume %}
                {% include 'CoreCoreBundle:Commande:detailpanier.html.twig' %}
            {% endblock resume%}
        {% endif %}
    </div>
    <div class="container-fluid">
        {% block footer %}
        {% include 'CoreCoreBundle:Default:footer.html.twig' %}
        {% endblock footer %}
     </div>



{% endblock body %}
{% block javascripts %}
    {{ parent() }}
    <script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyAE6dCPBTnGEsKzpyim81lzA7cYanfqP8U"
 async ></script> 
 <script defer src="{{ asset('js/Front/Commande/addressModifer.js') }}"></script>
 <script defer type="text/javascript" src="{{ asset('js/Default/ajax.js') }}"></script>
    <script defer src="https://www.paypalobjects.com/api/checkout.js"></script>
    <script defer src="{{ asset('js/Payement/paypal.js') }}"></script>

{% endblock javascripts %}
