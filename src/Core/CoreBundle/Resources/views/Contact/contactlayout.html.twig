{% extends '::base.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}"/>

{% endblock stylesheets %}


{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

    {{ parent() }}
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAE6dCPBTnGEsKzpyim81lzA7cYanfqP8U" charset="utf-8"></script>

    <script src="{{ asset('js/Front/Contact/map.js') }}"></script>

{% endblock javascripts %}

{% block body %}
    
    <div class="container-fluid">
        {% block header %}
        {% include 'CoreCoreBundle:Default:header.html.twig' %}
        {% endblock header %}
    </div>
    <div class="container">
        {% block formContact %}
        {% if is_granted('ROLE_USER')  %}
            {% include 'CoreCoreBundle:Contact:formcontactlog.html.twig' %}
        {% else %}
            {% include 'CoreCoreBundle:Contact:formcontact.html.twig' %}
        {% endif %}
        {% endblock formContact %}
        {% block map %}
            {% include 'CoreCoreBundle:Contact:map.html.twig' %}
        {% endblock map %}
    </div>
    <div class="container-fluid">
      	{% block footer %}
        {% include 'CoreCoreBundle:Default:footer.html.twig' %}
        {% endblock footer %}
     </div>
{% endblock body %}
