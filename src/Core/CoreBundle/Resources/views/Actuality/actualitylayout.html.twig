{% extends '::base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css') }}"/>

{% endblock stylesheets %}


{% block javascripts %}
    {{ parent() }}

    <script src="{{ asset('js/salvattore.min.js') }}"></script>
    <script src="{{ asset('js/scrollinfinity.min.js') }}"></script>


{% endblock javascripts %}

{% block body %}

    <div class="container-fluid">
        {% block header %}
            {% include 'CoreCoreBundle:Default:header.html.twig' %}
        {% endblock header %}
    </div>
    <div class="container">
        {% block actuality %}
            {% include 'CoreCoreBundle:Actuality:actuality.html.twig' %}
        {% endblock actuality %}
    </div>
    <div class="container-fluid">
     {% block footer %}
        {% include 'CoreCoreBundle:Default:footer.html.twig' %}
    {% endblock footer %}
</div>


{% endblock body %}
